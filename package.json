{
  "name": "my_portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192 --no-experimental-fetch\" next build",
    "start": "next start",
    "build:prod": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192 --no-experimental-fetch\" next build && pm2 start ecosystem.config.js",
    "pm2:start": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192 --no-experimental-fetch\" next build && pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop my-portfolio",
    "pm2:restart": "pm2 restart my-portfolio",
    "pm2:delete": "pm2 delete my-portfolio",
    "pm2:logs": "pm2 logs my-portfolio",
    "pm2:status": "pm2 status",
    "docker:build": "docker build -t my-portfolio:latest .",
    "docker:run": "docker run -p 1206:1206 my-portfolio:latest",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:restart": "docker-compose restart",
    "docker:logs": "docker-compose logs -f",
    "docker:ps": "docker-compose ps",
    "docker:build-up": "docker-compose up -d --build",
    "docker:clean": "docker-compose down && docker system prune -f",
    "lint": "eslint"
  },
  "dependencies": {
    "next": "16.0.6",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "16.0.6",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
